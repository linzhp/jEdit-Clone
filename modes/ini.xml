<?XML VERSION="1.0"?>

<!DOCTYPE MODE>

<MODE NAME="ini">
	<PROPS>
		<PROPERTY NAME="label" VALUE="MS-DOS INI [XMode]" />
		<PROPERTY NAME="blockComment" VALUE=";" />
		<PROPERTY NAME="filenameGlob" VALUE="*.{ini,reg}" />
	</PROPS>
	<RULES>
		<WHITESPACE> </WHITESPACE>
		<WHITESPACE>	</WHITESPACE>
		<SPAN TYPE="KEYWORD2" AT_LINE_START="TRUE">
			<BEGIN>[</BEGIN>
			<END>]</END>
		</SPAN>
		<EOL_SPAN TYPE="COMMENT1" AT_LINE_START="TRUE">;</EOL_SPAN>
		<MARK_PREVIOUS TYPE="KEYWORD1"
			AT_LINE_START="TRUE" EXCLUDE_MATCH="TRUE">=</MARK_PREVIOUS>
	</RULES>
</MODE>
