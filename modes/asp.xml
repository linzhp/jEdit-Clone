<?XML VERSION="1.0"?>

<!DOCTYPE MODE>

<MODE NAME="asp">
	<PROPS>
		<PROPERTY NAME="label" VALUE="ASP (JScript)" />
		<PROPERTY NAME="commentStart" VALUE="&lt;!--" />
		<PROPERTY NAME="commentEnd" VALUE="--&gt;" />
		<PROPERTY NAME="filenameGlob" VALUE="*.asp" />
		<PROPERTY NAME="firstlineGlob" VALUE="<%@" />
	</PROPS>
	<RULES IGNORE_CASE="TRUE">
		<!-- whitespace: (space and tab) -->
		<WHITESPACE> </WHITESPACE>
		<WHITESPACE>	</WHITESPACE>

		<SPAN TYPE="LABEL" DELEGATE="javascript::MAIN">
			<BEGIN>&lt;%</BEGIN>
			<END>%&gt;</END>
		</SPAN>

		<SPAN TYPE="LABEL" DELEGATE="javascript::MAIN">
			<BEGIN>&lt;%=</BEGIN>
			<END>%&gt;</END>
		</SPAN>

		<SPAN TYPE="LABEL" DELEGATE="javascript::MAIN">
			<BEGIN>&lt;SCRIPT&gt;</BEGIN>
			<END>&lt;/SCRIPT&gt;</END>
		</SPAN>

		<!-- stylesheet -->
		<SPAN TYPE="LABEL" DELEGATE="css::MAIN">
			<BEGIN>&lt;STYLE&gt;</BEGIN>
			<END>&lt;/STYLE&gt;</END>
		</SPAN>

		<!-- SGML comment -->
		<SPAN TYPE="COMMENT1">
			<BEGIN>&lt;!--</BEGIN>
			<END>--&gt;</END>
		</SPAN>

		<!-- markup tags -->
		<SPAN TYPE="KEYWORD1" DELEGATE="TAGS">
			<BEGIN>&lt;</BEGIN>
			<END>&gt;</END>
		</SPAN>

		<!-- SGML entities -->
		<SPAN TYPE="LITERAL2" NO_WORD_BREAK="TRUE">
			<BEGIN>&amp;</BEGIN>
			<END>;</END>
		</SPAN>
	</RULES>

	<RULES SET="TAGS" DEFAULT="KEYWORD1">
		<!-- whitespace: (space and tab) -->
		<WHITESPACE> </WHITESPACE>
		<WHITESPACE>	</WHITESPACE>

		<SPAN TYPE="LABEL" DELEGATE="javascript::MAIN">
			<BEGIN>&lt;%</BEGIN>
			<END>%&gt;</END>
		</SPAN>

		<SPAN TYPE="LABEL" DELEGATE="javascript::MAIN">
			<BEGIN>&lt;%=</BEGIN>
			<END>%&gt;</END>
		</SPAN>

		<SPAN TYPE="LITERAL1">
			<BEGIN>"</BEGIN>
			<END>"</END>
		</SPAN>

		<SPAN TYPE="LITERAL1">
			<BEGIN>'</BEGIN>
			<END>'</END>
		</SPAN>
	</RULES>
</MODE>
